X5_Traits <- filter(X5_Traits, !identifiant.participant %in% c(9, 13, 33, 23, 8, 36, 54, 27))
# X5_Traits <- filter(X5_Traits, a.quelle.frequence.jouez.vous.aux.jeux.videos.. != "Jamais")
# Erase double from data
