DFS_TPI <- mutate(rowwise(DFS_TPI), action_awareness=sum(c_across(seq.int(from=3, length.out=4, by=9))))
DFS_TPI <- mutate(rowwise(DFS_TPI), clear_goals=sum(c_across(seq.int(from=4, length.out=4, by=9))))
