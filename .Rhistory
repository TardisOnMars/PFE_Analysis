DFS_TPI <- mutate(rowwise(DFS_TPI), time_transformation=sum(c_across(seq.int(from=9, length.out=4, by=9))))
DFS_TPI <- mutate(rowwise(DFS_TPI), autotelic_experience=sum(c_across(seq.int(from=10, length.out=4, by=9))))
